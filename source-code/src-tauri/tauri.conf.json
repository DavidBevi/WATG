{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "watg",
  "version": "0.4.0",
  "identifier": "davidbevi.watg",
  "app": {
    "withGlobalTauri": true,
    "security": {
      "csp": "default-src 'self'; connect-src ipc: http://ipc.localhost"
    }
  },
  "plugins": {
    "pluginReportTitle": {
      "invoke": true
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "src/icons/icon-watg.svg",
      "src/icons/icon-watg.png",
      "src/icons/icon-watg.ico",
      "src/icons/icon-watg-0.png",
      "src/icons/icon-watg-1.png",
      "src/icons/icon-watg-2.png",
      "src/icons/icon-watg-3.png",
      "src/icons/icon-watg-4.png",
      "src/icons/icon-watg-5.png",
      "src/icons/icon-watg-6.png",
      "src/icons/icon-watg-7.png",
      "src/icons/icon-watg-8.png",
      "src/icons/icon-watg-9.png",
      "src/icons/icon-watg-10.png"
    ]
  }
}

